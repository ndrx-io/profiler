{
    "name": "ndrx/profiler",
    "description": "Simple profiler package",
    "keywords": [
        "ndrx",
        "profiler"
    ],
    "homepage": "https://github.com/ndrx-io/profiler",
    "license": "MIT",
    "authors": [
        {
            "name": "LAHAXE Arnaud",
            "email": "contact@ndrx.io",
            "homepage": "http://ndrx.io/fr/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "php-jsonpatch/php-jsonpatch": "1.2.*",
        "symfony/http-foundation": "^2.7",
        "symfony/finder": "^2.7",
        "symfony/filesystem": "^2.7",
        "symfony/event-dispatcher": "^2.7",
        "psr/log": "^1.0",
        "symfony/options-resolver": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "monolog/monolog": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Ndrx\\Profiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ndrx\\Profiler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
