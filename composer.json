{
    "name": "ndrx/profiler",
    "description": "Simple profiler package",
    "keywords": [
        "ndrx",
        "profiler"
    ],
    "homepage": "https://github.com/ndrx-io/profiler",
    "license": "MIT",
    "authors": [
        {
            "name": "LAHAXE Arnaud",
            "email": "contact@ndrx.io",
            "homepage": "http://ndrx.io/fr/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Ndrx\\Profiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ndrx\\Profiler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
